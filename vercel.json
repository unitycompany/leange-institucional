{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" },
      { "source": "/metrics/gtm.js", "destination": "https://www.googletagmanager.com/gtm.js" },
      { "source": "/metrics/ns.html", "destination": "https://www.googletagmanager.com/ns.html" },
      { "source": "/metrics/g/collect", "destination": "https://www.google-analytics.com/g/collect" },
      { "source": "/metrics/pagead/1p-conversion/(.*)", "destination": "https://googleads.g.doubleclick.net/pagead/1p-conversion/$1" },
      { "source": "/metrics/j/collect", "destination": "https://www.google-analytics.com/j/collect" }
    ]
  }
  